[Credentials]
; 账号
username =
; 密码
password =

[Notification]
; 是否启用定时任务
enable_timing = true
; 可选平台：localfile, feishu, serverchan, mqtt, webhook, bark。多个平台启用时用逗号隔开
platform = LocalFile
; 设定的通知时间
time = 12:00:00

[ServerChan]
; Server酱发送密钥
send_key = your_serverchan_send_key

[Feishu]
; 飞书机器人token
robot_token = your_feishu_send_key

[Webhook]
; Webhook URL
webhook_url =

[MQTT]
; Mqtt的地址
mqtt_host = your_mqtt_host(example: 127.0.0.1 or mqtt.com)
mqtt_port = your_mqtt_port(example: 1883)
mqtt_topic = your_mqtt_topic(example: /me/electricity)

[Bark]
url =
token =

[LocalFile]
; 保留原有的http服务
file_path =